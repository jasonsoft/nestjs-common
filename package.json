{
  "name": "@jasonsoft/nestjs-common",
  "version": "1.1.20",
  "description": "Nestjs common extension for Nest framework (node.js)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonsoft/nestjs-common.git"
  },
  "keywords": [
    "@jasonsoft/nestjs-common",
    "jasonsoft",
    "nestjs-common",
    "nestjs",
    "common",
    "typeorm",
    "pagination"
  ],
  "author": "Jason Song (成长的小猪)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsoft/nestjs-common/issues"
  },
  "homepage": "https://github.com/jasonsoft/nestjs-common#readme",
  "devDependencies": {
    "@nestjs/common": "9.2.0",
    "@types/bcrypt": "5.0.0",
    "@types/js-yaml": "4.0.5",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "typeorm": "0.3.10",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "class-transformer": ">=0.2.3",
    "class-validator": ">=0.10.0",
    "rxjs": ">=6.0.0",
    "typeorm": "^0.2.7 || ^0.3.0",
    "uuid": "^8.0.0 || ^9.0.0"
  },
  "dependencies": {
    "bcrypt": "5.1.0",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "8.5.1"
  }
